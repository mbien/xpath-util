                    <file name="net-java-xpath-completion-XPathCompletionProvider.instance"/>
                </folder>
                <file name="EditorKit.instance">
                    <attr name="instanceClass"
                    stringvalue="net.java.xpath.XPathEditorKit"/>
                </file>
            </folder>
        </folder>
    </folder>

    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="XPathResolver.xml" url="XPathResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="net.java.xpath.Bundle"/>
