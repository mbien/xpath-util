<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

    <folder name="Actions">
        <folder name="Window">
            <file name="net-java-xpath-ui-XPathAction.instance">
                <attr name="component" methodvalue="net.java.xpath.ui.XPathTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="net.java.xpath.Bundle#Actions/Window/net-java-xpath-ui-XPathAction.instance"/>
                <attr name="iconBase" stringvalue="net/java/xpath/ui/utilities-terminal.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>

    <folder name="Menu">
        <folder name="Window">
            <file name="XPathAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/net-java-xpath-ui-XPathAction.instance"/>
                <attr name="position" intvalue="850"/>
            </file>
        </folder>
    </folder>

    <folder name="Editors">
        <folder name="text">
            <folder name="x-xpath">
                <folder name="CompletionProviders">
                    <file name="net-java-xpath-completion-XPathCompletionProvider.instance"/>
                </folder>
                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="net.java.xpath.XPathEditorKit"/>
                </file>
            </folder>
        </folder>
    </folder>

    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="XPathResolver.xml" url="XPathResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="net.java.xpath.Bundle"/>
            </file>
        </folder>
    </folder>

    <folder name="Windows2">
        <folder name="Components">
            <file name="XPathTopComponent.settings" url="XPathTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="output">
                <file name="XPathTopComponent.wstcref" url="XPathTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>

    <folder name="Windows2">
        <folder name="Components">
            <file name="XPathTopComponent.settings" url="XPathTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="output">
                <file name="XPathTopComponent.wstcref" url="XPathTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>

</filesystem>
